{
    "expo": {
      "name": "TamagotchiApp",
      "slug": "tamagotchiapp",
      "version": "1.0.0",
      "sdkVersion": "48.0.0",
      "platforms": ["ios", "android", "web"],
      "assets": ["./assets"],
      "expo": {
    "plugins": [
      "expo-some-plugin"
    ]
    },
      "entryPoint": "./App.js",
      "orientation": "portrait",
      "userInterfaceStyle": "light",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "ios": {
        "bundleIdentifier": "com.example.tamagotchiapp",
        "supportsTablet": true
      },
      "android": {
        "package": "com.example.tamagotchiapp",
        "versionCode": 1
      }
      
    }
  }